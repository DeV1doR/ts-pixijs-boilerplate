{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "use-strict": [
            true,
            "check-module",
            "check-function"
        ],
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
    },
    "rulesDirectory": []
}